{
	"schema": {
		"bsonType": "object",
		"required": [
			"category_id",
			"menu_id",
			"title"
		],
		"properties": {
			"_id": {
				"description": "存储文档 ID（商品 ID），系统自动生成"
			},
			"category_id": {
				"bsonType": "string",
				"description": "题库分类 id"
			},
			"menu_id": {
				"bsonType": "string",
				"description": "题目所属菜单",
				"title": "菜单"
			},
			"title": {
				"bsonType": "string",
				"description": "题目标题描述",
				"title": "标题"
			},
			"keywords": {
				"bsonType": "string",
				"description": "题目关键字，用于检索",
				"title": "关键字"
			},
			"add_date": {
				"bsonType": "timestamp",
				"description": "添加时间",
				"defaultValue": {
					"$env": "now"
				}
			},
			"last_modify_date": {
				"bsonType": "timestamp",
				"description": "最后修改时间",
				"defaultValue": {
					"$env": "now"
				}
			}
		}
	},
	"data": [],
	"index": []
}
